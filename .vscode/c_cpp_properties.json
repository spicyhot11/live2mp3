{
    "configurations": [
        {
            "name": "macOS",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/backend",
                "${workspaceFolder}/backend/controllers",
                "${workspaceFolder}/backend/services",
                "${workspaceFolder}/backend/utils",
                "/opt/homebrew/include"
            ],
            "defines": [
                "__APPLE__",
                "__MACH__"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/backend/build/Debug/compile_commands.json"
        }
    ],
    "version": 4
}